{
    "name": "roshangautam/sentinel-ldap",
    "type": "platform-extension",
    "description": "Adds authentication via LDAP to Cartalyst Sentinel",
    "authors": [
        {
            "name": "Roshan Gautam",
            "email": "roshan.gautam@hotmail.edu"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.cartalyst.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "cartalyst/composer-installers": "^2.0",
        "cartalyst/support": "^2.0",
        "platform/foundation": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Roshangautam\\Sentinel\\Addons\\Ldap\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
